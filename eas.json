{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "REACT_APP_ENV": "production",
        "REACT_APP_API_URL": "https://lpbackend-w3g7.onrender.com",
        "REACT_APP_DEBUG": "false"
      }
    },
    "debug": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "REACT_APP_ENV": "development",
        "REACT_APP_DEBUG": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
